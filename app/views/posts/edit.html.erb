<h1>Editing Post</h1>

<%= form_for(@post) do |f| %>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :title %><br>
    <%= f.text_field :title %><br>
    <%= f.label :content %><br>
    <%= f.text_field :content%>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
<%= link_to 'Show', @post %> |
<%= link_to 'Back', posts_path %>
